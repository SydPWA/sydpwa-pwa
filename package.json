    "preact": "^8.1.0",
    "preact-async-route": "^1.0.3",
    "preact-compat": "^3.16.0",
    "preact-markup": "^1.6.0",
    "preact-render-to-string": "^3.6.3",
    "preact-router": "^2.5.2",
    "preact-scroll-header": "^1.0.1",
    "promise-polyfill": "^6.0.2",
    "proptypes": "^1.0.0",
    "unfetch": "^2.1.2",
    "yaml": "^0.3.0"
  },
  "main": "webpack.config.babel.js",
  "directories": {
    "test": "test"
  }
}
