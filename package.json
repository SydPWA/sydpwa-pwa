{
  "name": "sydpwa-web",
  "version": "1.0.0",
  "description": "Ready-to-go Preact starter project powered by webpack.",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress",
    "start": "serve build -p8080 -s -c 1",
    "prestart": "npm run build",
    "build": "cross-env NODE_ENV=production webpack -p --progress",
    "prebuild": "rm -rf build && mkdirp build && cp -r src/assets build/assets",
    "test": "npm run -s lint && npm run -s test:karma",
    "test:karma": "karma start test/karma.conf.js --single-run",
    "lint": "eslint {src,test}",
    "format": "prettier-eslint --write \"src/**/*.js\"",
    "deploy": "npm run build && firebase deploy"
  },
  "keywords": [
    "preact",
    "boilerplate",
    "webpack"
  ],
  "license": "MIT",
  "author": "Zouhir Chahoud - <zouhir@zouhir.org>",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "chai": "^4.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "ejs-loader": "^0.3.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-z3": "^0.2.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "mkdirp": "^0.5.1",
    "mocha": "^3.4.2",
    "ncp": "^2.0.0",
    "node-sass": "^4.5.3",
    "offline-plugin": "^4.8.1",
    "postcss-loader": "^2.0.5",
    "postcss-scss": "^1.0.0",
    "prettier-eslint-cli": "^3.6.1",
    "raw-loader": "^0.5.1",
    "replace-bundle-webpack-plugin": "^1.0.0",
    "sass-loader": "^6.0.5",
    "script-ext-html-webpack-plugin": "^1.8.1",
    "serve": "^5.2.2",
    "sinon": "^2.3.2",
    "sinon-chai": "^2.10.0",
    "source-map-loader": "^0.2.1",
    "standard": "^10.0.2",
    "style-loader": "^0.18.2",
    "svg-url-loader": "^2.0.2",
    "url-loader": "^0.5.8",
    "v8-lazy-parse-webpack-plugin": "^0.3.0",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "cors": "^2.8.3",
    "css-modules-require-hook": "^4.0.6",
    "decko": "^1.2.0",
    "eslint-config-z3": "^0.2.1",
    "express": "^4.15.3",
